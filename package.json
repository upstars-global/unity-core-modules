{
  "name": "unity-core-modules",
  "version": "1.72.0",
  "description": "",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/upstars-global/unity-core-modules.git"
  },
  "engines": {
      "node": ">=22.22.0"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint --max-warnings=8",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "ci": "find . -type d -name \"node_modules\" -prune -exec rm -rf {} + && yarn cache clean && yarn",
    "depcruise:test": "depcruise src --include-only '^src'",
    "depcruise:svg": "depcruise --include-only '^src' --output-type dot src | dot -T svg > dependencygraph.svg",
    "depcruise:check": "node scripts/depcruise-threshold.mjs --config=.dependency-cruiser.cjs --maxErrors=0 --maxWarnings=99"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  },
  "private": true,
  "dependencies": {
    "@gtm-support/vue-gtm": "^3.2.0",
    "@sentry/vue": "^8.55.0",
    "buffer": "^6.0.3",
    "centrifuge": "1.3.7",
    "dayjs": "^1.11.19",
    "pinia": "^3.0.4",
    "recaptcha-v3": "^1.11.3",
    "tiny-emitter": "^2.1.0",
    "typescript": "^5.9.3",
    "ua-parser-js": "^2.0.9",
    "uuid": "^9.0.1",
    "vue": "^3.5.28",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@octokit/auth-app": "^8.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "22.19.11",
    "@types/uuid": "^9.0.8",
    "@vitest/coverage-v8": "^3.2.4",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "dependency-cruiser": "^17.3.8",
    "eslint": "^9.39.2",
    "eslint-config-frontera": "upstars-global/eslint-config-frontera#v2.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "semantic-release": "^25.0.3",
    "vite": "^5.4.21",
    "vitest": "^3.2.4"
  }
}